{
  "name": "svn-compare",
  "version": "1.0.0",
  "description": "SVN文件对比工具",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "vite build && electron-builder"
  },
  "dependencies": {
    "vue": "^3.4.0",
    "monaco-editor": "^0.45.0",
    "element-plus": "^2.4.0",
    "iconv-lite": "^0.6.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.0",
    "vite": "^5.0.0",
    "concurrently": "^8.2.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.svn.compare",
    "productName": "SVN Compare",
    "win": {
      "target": "nsis"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ]
  }
}
